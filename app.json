{
  "expo": {
    "name": "GlucoCare",
    "slug": "glucocare",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "glucocare",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eduabjr.glucocare"
    },
    "android": {
      "package": "com.eduabjr.glucocare",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "minSdkVersion": 24
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ],
      "expo-local-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "bb849c2-0bbc-4d34-ac66-441748aa2550"
      },
      "expoClientId": "960817041807-80cgl0vv08sls7dhlhngd87lehld7app.apps.googleusercontent.com"
    }
  }
}
