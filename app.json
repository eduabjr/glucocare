{
  "expo": {
    "name": "GlucoCare",
    "slug": "glucocare",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "glucocare",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eduardofamilia01.glucocare",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow $(PRODUCT_NAME) to use Face ID",
        "NSBluetoothAlwaysUsageDescription": "Allow $(PRODUCT_NAME) to connect to glucose meters via Bluetooth",
        "NSBluetoothPeripheralUsageDescription": "Allow $(PRODUCT_NAME) to connect to glucose meters via Bluetooth",
        "NSLocationWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to scan for Bluetooth devices",
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to save reports to your photo library",
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "CSV Files",
            "CFBundleTypeRole": "Viewer",
            "LSHandlerRank": "Owner",
            "LSItemContentTypes": [
              "public.comma-separated-values-text"
            ]
          },
          {
            "CFBundleTypeName": "Excel Files",
            "CFBundleTypeRole": "Viewer",
            "LSHandlerRank": "Owner",
            "LSItemContentTypes": [
              "org.openxmlformats.spreadsheetml.sheet",
              "com.microsoft.excel.xls"
            ]
          },
          {
            "CFBundleTypeName": "XML Files",
            "CFBundleTypeRole": "Viewer",
            "LSHandlerRank": "Owner",
            "LSItemContentTypes": [
              "public.xml",
              "public.text"
            ]
          },
          {
            "CFBundleTypeName": "PDF Files",
            "CFBundleTypeRole": "Viewer",
            "LSHandlerRank": "Owner",
            "LSItemContentTypes": [
              "com.adobe.pdf"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.eduardofamilia01.glucocare",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "file",
              "mimeType": "text/csv"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "file",
              "mimeType": "application/vnd.ms-excel"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "file",
              "mimeType": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "file",
              "mimeType": "application/xml"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "file",
              "mimeType": "text/xml"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "file",
              "mimeType": "application/pdf"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-local-authentication",
      "expo-font",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to scan for Bluetooth devices."
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "2d82c0cb-e0fd-40f3-bb50-abd697fa4e8d"
      },
      "expo": {
        "owner": "eduardofamilia01-hub"
      },
      "//": "ID de Cliente para o build nativo iOS. Crie uma credencial 'iOS' se for buildar para iPhone/iPad.",
      "expoClientId": "360317541807-i8qgcvkit3vsv8s7did5rgjod17eld77.apps.googleusercontent.com",
      "webClientId": "360317541807-i8qgcvkit3vsv8s7did5rgjod17eld77.apps.googleusercontent.com",
      "androidClientId": "360317541807-19cbu2121eftbm4d9p50mk3okma4bhtj.apps.googleusercontent.com",
      "iosClientId": "360317541807-i8qgcvkit3vsv8s7did5rgjod17eld77.apps.googleusercontent.com"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}
